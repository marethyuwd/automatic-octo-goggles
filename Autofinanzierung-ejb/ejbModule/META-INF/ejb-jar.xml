<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/ejb-jar_3_2.xsd"
	version="3.2">
	<display-name>Autofinanzierung-ejb</display-name>
	<enterprise-beans>
		<session>
			<ejb-name>LoanCalculatorBean</ejb-name>
			<env-entry>
				<env-entry-name>interestRate</env-entry-name>
				<env-entry-type>java.lang.Double</env-entry-type>
				<env-entry-value>0.01</env-entry-value>
			</env-entry>
		</session>
	</enterprise-beans>
	<ejb-client-jar>Autofinanzierung-common.jar</ejb-client-jar>
</ejb-jar>